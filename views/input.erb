<h1><%= @doctor.name %></h1>





<form class="patientForm" action="/doctors/<%= @doctor.id %>" method="post">
  <div class="form-group">
    <label for="name">Enter Name of Patient:</label>
    <input type="text" name="name" placeholder="'John'" required>
  </div>
  <div class="form-group">
    <label for="dob">Enter Patient Birthday:</label>
    <input type="date" name="dob" placeholder="'XXXX-XX-XX'" required>
  </div>

  <button type="submit" class="btn btn-info">Submit</button>
</form><br>

<div class="container">
  <a href="/admin_home"><button class="btn btn-info">Home</a>
</div>
